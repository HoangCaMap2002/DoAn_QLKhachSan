    <!--=============== scripts  ===============-->
    <script type="text/javascript" src="../assets/js/jquery.min.js"></script>
    <script type="text/javascript" src="../assets/js/plugins.js"></script>
    <script type="text/javascript" src="../assets/js/scripts.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBOYdeXNSuANfFSi3EY9-NaqxT_MHxHX3c&libraries=places&amp&callback=initAutocomplete"></script>
    <script type="text/javascript" src="../assets/js/map-single.js"></script>
    <script>
        $(document).ready(function () {
            $("#searchHeader").on("click", function () {
                // Lấy giá trị từ các trường input
                var date = $("input[name='header-search']").val();
                var dateArray = date.split(" - ");
                var startDate = dateArray[0];
                var endDate = dateArray[1];
                // Gửi dữ liệu đến controller
                $.ajax({
                    type: "GET",
                    url: "/KhachSan/KiemTraKhachSanConPhong",
                    data: {
                        start: startDate,
                        end: endDate
                    },
                    success: function (response) {
                        // Xử lý kết quả thành công
                        $("#Result").html(response);
                    },
                    error: function (error) {
                        console.log("Error:", error);
                    }
                });
            });
            $("#searchBody").on("click", function () {
                // Lấy giá trị từ các trường input
                var date = $("input[name='dates']").val();
                var selectCity = $(".listsearch-input-item select").val();
                if (date != "") {
                    var dateArray = date.split(" - ");
                    var startDate = dateArray[0];
                    var endDate = dateArray[1];
                    // Gửi dữ liệu đến controller
                    $.ajax({
                        type: "GET",
                        url: "/KhachSan/KiemTraKhachSanConPhong",
                        data: {
                            start: startDate,
                            end: endDate,
                            city: selectCity
                        },
                        success: function (response) {
                            // Xử lý kết quả thành công
                            $("#Result").html(response);
                        },
                        error: function (error) {
                            console.log("Error:", error);
                        }
                    });
                }
                else {
                    alert("Vui lòng chọn ngày check-in và check-out");
                }
            });

        });
    </script>
</body>

